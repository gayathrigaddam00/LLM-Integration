{
    "manifest_version": 3,
    "name": "HTML Extractor Sel",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Extract HTML"
    },
    "host_permissions": ["<all_urls>"]
}
